<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImageCropView">
        <attr name="icv_grid_inner_stroke" format="dimension" />
        <attr name="icv_grid_inner_color" format="color" />
        <attr name="setInnerGridMode" format="integer">
            <enum name="off" value="0" />
            <enum name="on" value="1" />
        </attr>
        <attr name="icv_grid_Outer_stroke" format="dimension" />
        <attr name="gridOuterColor" format="color" />
        <attr name="setOuterGridMode" format="integer">
            <enum name="off" value="0" />
            <enum name="on" value="1" />
        </attr>
        <attr name="gridLeftRightMargin" format="dimension" />
        <attr name="gridTopBottomMargin" format="dimension" />
    </declare-styleable>

</resources>
